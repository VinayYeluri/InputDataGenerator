<UserControl x:Class="SIEFSimulatorInputGenerator.UserControls.VoiesCirculationPrevuesUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SIEFSimulatorInputGenerator.UserControls"
             mc:Ignorable="d" 
             Width="700" d:DesignHeight="40">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="161*"></ColumnDefinition>
            <ColumnDefinition Width="239*"></ColumnDefinition>
            <ColumnDefinition Width="161*"></ColumnDefinition>
            <ColumnDefinition Width="239*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Label Content="VoieSortie" Grid.Column="0" Margin="7,7,10.333,7"></Label>
        <Label Content="VoieVia" Grid.Column="2" Margin="7,7,10.667,7"></Label>

        <TextBox Name="_VoieSortie" Text="{Binding Path=VoieSortie, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Column="1" Grid.Row="0" Margin="3.667,7,7,7"></TextBox>
        <TextBox Name="_VoieVia" Text="{Binding Path=VoieVia, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Grid.Column="3" Grid.Row="0" Margin="3.333,7,7,7"></TextBox>
    </Grid>
</UserControl>
